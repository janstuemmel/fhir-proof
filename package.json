{
  "name": "fhir-proof",
  "version": "0.3.2",
  "description": "typed javascript classes for fhir",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./lib",
    "test:all": "npm test -- --coverage && npm run lint",
    "test:watch": "npm test -- --watch",
    "version:patch": "npm run test:all && npm version patch",
    "version:minor": "npm run test:all && npm version minor",
    "version:major": "npm run test:all && npm version major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janstuemmel/fhir-proof.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janstuemmel/fhir-proof/issues"
  },
  "homepage": "https://github.com/janstuemmel/fhir-proof#readme",
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.24.0",
    "codeclimate-test-reporter": "^0.4.1",
    "eslint": "^3.18.0",
    "jest": "^19.0.2"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/test/**/*Spec.js"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "tcomb": "^3.2.20"
  }
}
